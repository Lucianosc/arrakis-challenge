{
  "name": "se-2",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "format": "yarn next:format",
    "generate": "yarn workspace @se-2/nextjs generate",
    "next:build": "yarn workspace @se-2/nextjs build",
    "next:check-types": "yarn workspace @se-2/nextjs check-types",
    "next:format": "yarn workspace @se-2/nextjs format",
    "next:lint": "yarn workspace @se-2/nextjs lint",
    "next:serve": "yarn workspace @se-2/nextjs serve",
    "next:test": "yarn workspace @se-2/nextjs vitest",
    "next:test:watch": "yarn workspace @se-2/nextjs vitest watch",
    "next:test:ui": "yarn workspace @se-2/nextjs vitest --ui",
    "next:test:coverage": "yarn workspace @se-2/nextjs vitest run --coverage",
    "next:test:ci": "yarn workspace @se-2/nextjs vitest run",
    "postinstall": "husky install",
    "precommit": "lint-staged",
    "start": "yarn workspace @se-2/nextjs dev",
    "vercel": "yarn workspace @se-2/nextjs vercel",
    "vercel:yolo": "yarn workspace @se-2/nextjs vercel:yolo"
  },
  "devDependencies": {
    "husky": "~8.0.3",
    "lint-staged": "~13.2.2"
  },
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": ">=18.18.0"
  }
}
